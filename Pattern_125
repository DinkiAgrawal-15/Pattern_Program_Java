import java.util.*;
public class Main{
    public static void main(String a[]){
    int n=5;
    int i,j,k;
    for(i=n-1;i>=0;i--){
        for(j=n-1;j>i;j--){
                    System.out.printf(" ");
        }
        for(k=0;k<=i;k++){
                        System.out.printf("%2c",i+65);

        }

            
        System.out.printf("\n");
    }
}}
