import java.util.*;
public class Main{
    public static void main(String a[]){
    int n=5;
    int i,j,k;
    for(i=1;i<=n;i++){
        for(k=n-1;k>=i;k--){
                    System.out.printf(" ");
        }
        for(j=1;j<=i;j++){
                        System.out.printf("%2d",j);

        }


        System.out.printf("\n");
    }
}}
