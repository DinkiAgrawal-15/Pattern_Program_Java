import java.util.*;
public class Main{
    public static void main(String a[]){
    int n=5;
    int i,j,k;
    for(i=n;i>=1;i--){
        for(k=n-1;k>=i;k--){
                    System.out.printf(" ");
        }
        for(j=i;j>=1;j--){
                        System.out.printf("%2d",j);

        }


        System.out.printf("\n");
    }
}}
