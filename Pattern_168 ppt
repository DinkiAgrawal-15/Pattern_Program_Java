import java.util.*;
class Main{
    public static void main(String a[]){
        int i,j,n;
        Scanner sc=new Scanner(System.in);
        n=sc.nextInt();
        for(i=n;i>=1l;i--){
            for(j=1;j<=i;j++){
                System.out.printf("%d",i%2);
            }
            System.out.printf("\n");
        }
    }
}
