import java.util.*;
class Main{
   public static void main(String a[]){
    int n=5;
    int i,j,k;
    for(i=1;i<=n;i++){
        for(j=1;j<=n;j++){
            if(i<=j)
            System.out.printf("%d",i);
            else
            System.out.printf(" ");
        }
       
        
        System.out.printf("\n");
    }
}}
