import java.util.*;
class Main{
    public static void main(String a[]){
        int n=5;
        int i,j;
        int x=1,y,t;
        for(i=n;i>=1;i--){
            y=i;
            t=x;
            for(j=n;j>=i;j--){
                System.out.print(t);
                t=t-y;
                y++;
                
            }
            x=x+i;
            System.out.println("");
        }
    }
}
