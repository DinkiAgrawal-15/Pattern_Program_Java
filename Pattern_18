import java.util.*;
class Main{
    public static void main(String a[]){
       
        int i ,j;
        int n=5;
        for(i=1;i<=n;i++){
            
            for(j=1;j<=n;j++){
               
                                    System.out.print((i+j)%2);

                }
              
            System.out.println("\n");
        }
        
    }
}
