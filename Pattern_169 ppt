import java.util.*;
class Main{
    public static void main(String a[]){
        int i,j,n;
        Scanner sc=new Scanner(System.in);
        n=sc.nextInt();
        for(i=n;i>=1;i--){
            for(j=i;j>=1;j--){
                System.out.printf("%d",j);
            }
            System.out.printf("\n");
        }
    }
}
