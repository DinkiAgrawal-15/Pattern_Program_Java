import java.util.*;
class Main{
    public static void main(String a[]){

    int n=5;
    int i,j,k;
    for(i=0;i<n;i++){
        for(j=n-1;j>i;j--){
            System.out.printf(" ");
        }
        for(k=0;k<=i;k++){
            System.out.printf("%c",k+65);
        }
        System.out.printf("\n");
    }
}}
