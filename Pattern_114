import java.util.*;
class Main{
    public static void main(String a[]){

    int n=5;
    int i,j,k;
    for(i=0;i<n;i++){
        for(j=0;j<n-i;j++){
            System.out.printf(" ");
        }
        for(k=i;k>=0;k--){
            System.out.printf("%c",k+65);
        }
        System.out.printf("\n");
    }
}}
