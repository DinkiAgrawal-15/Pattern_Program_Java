import java.util.*;
class Main{
    public static void main(String a[]){
        int n=5;
        int x;
        for(int i=1;i<=n;i++){
            x=n-i;
            for(int j=1;j<=n;j++){
                System.out.print((char)(x+65)+" ");
                x=x+n;
            }
            System.out.println();
        }
    }
}
