import java.util.*;
class Main{
    public static void main(String a[]){
        int i,j,n,k=1;
        Scanner sc=new Scanner(System.in);
        n=sc.nextInt();
        for(i=1;i<=n;i++){
            for(j=1;j<=i;j++){
                System.out.printf("%d",k++);
            }
            System.out.printf("\n");
        }
    }
}
